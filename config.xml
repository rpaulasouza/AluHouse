<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.aluhouse" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TesteMapa</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <plugin name="cordova-plugin-googlemaps" spec="^1.4.3">
        <variable name="API_KEY_FOR_ANDROID" value="AIzaSyB26zDb9n6KrXcMQfskOkPX4tQgL-_RKY8" />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
